<div class="col-md-10 p-5">
  <table class="table table-bordered table-striped">
    <thead>
      <tr class="text-center">
        <th class="col-md-1" scope="col">No</th>
        <th class="col-md-3" scope="col">Food Name</th>
        <th class="col-md-2" scope="col">Category</th>
        <th class="col-md-4" scope="col">Ingredients</th>
      </tr>
    </thead>
    <tbody>
      <% for (let i=0; i < search.length; i++) { %>
      <tr>
        <th class="text-center" scope="row"><%= i+1 %></th>

        <td><%= search[i].foodName %></td>
        <td><%= search[i].Category.categoryName %></td>
        <td><%= search[i].ingredients %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</div>
